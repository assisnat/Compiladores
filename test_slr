S -> INICIAL

INICIAL -> EXP SepPV
INICIAL -> DEC INICIAL
INICIAL -> MODULE INICIAL
INICIAL -> SepPV INICIAL

DEC -> val TVARS PAT OpAtr EXP CONTDEC
DEC -> val PAT OpAtr EXP CONTDEC
CONTDEC -> And PAT OpAtr EXP CONTDEC
DEC -> fun TVARS FBIND CONTFBIND
DEC -> fun FBIND CONTFBIND
CONTFBIND -> And FBIND CONTFBIND
DEC -> Type TBINDS
DEC -> infix ConstInt Id CONTID
DEC -> infix Id CONTID
DEC -> nonfix Id CONTID
CONTID -> Id CONTID

TVARS -> TVAR
TVARS -> BeginP TVAR CONTTVARS EndP
CONTTVARS -> SepV TVAR CONTTVARS

TBINDS -> TVARS Id OpAtr TYPE CONTTBINDS
TBINDS -> Id OpAtr TYPE CONTTBINDS
CONTTBINDS -> And TVARS Id OpAtr TYPE CONTTBINDS
CONTTBINDS -> Id OpAtr TYPE CONTTBINDS

FBIND -> Id APAT CONTAPAT OpAtr EXP CONTFBINDD
CONTAPAT -> APAT CONTAPAT
CONTFBINDD -> Bar Id APAT CONTAPAT OpAtr EXP CONTFBINDD

EXP -> TypeConst
EXP -> PREFIXFN 
EXP -> PREFCON
EXP -> BeginP EXP CONTEXP EndP
CONTEXP -> SepV EXP CONTEXP 
EXP -> BeginP EXP CONTEXP EndP
CONTEXP -> SepPV EXP CONTEXP
EXP -> BeginC EXP CONTEXP EndC
CONTEXP -> SepV EXP CONTEXP 
EXP -> OpLogAndAlso EXP    
EXP -> OpLogOr EXP
EXP -> InsSIf EXP InsSThen EXP InsSElse EXP
EXP -> InsInWh EXP InsInDo EXP
EXP -> InsInCase EXP Of MATCH
EXP -> Fn MATCH

MATCH -> PAT Arrow EXP CONTMATCH
CONTMATCH -> EXP Bar PAT Arrow EXP
PAT -> APAT           # GERA AMBIGUIDADE
PAT -> PREFCON PAT
PAT -> INFIXCON PAT
APAT -> TypeConst 
APAT -> INFIXCON 
APAT -> BeginP PAT EndP     # GERA AMBIGUIDADE
APAT -> BeginP APAT EndP
APAT -> BeginC EndC APAT
APAT -> BeginC PAT EndC

INFIXFN -> OpArMult
INFIXFN -> OpArdiv
INFIXFN -> Div
INFIXFN -> Mod
INFIXFN -> OpArAd
INFIXFN -> OpArExp
INFIXFN -> At
INFIXFN -> OpAtr
INFIXFN -> OpReI
INFIXFN -> OpReD
INFIXFN -> Id

PREFIXFN -> op INFIXCON
PREFIXFN -> OpMinus
PREFIXFN -> OpLogNot
PREFIXFN -> Abs

INFIXCON -> DoubleP
INFIXCON -> Id

MONOCON -> ConstBool

TYPE -> TypeInt 
TYPE -> TypeChar
TYPE -> TypeString 
TYPE -> TypeBool
TYPE -> List

TVAR -> Asp Id